# GOPATH
layout go

# MAKEPATH
export PATH=$PATH:$GOPATH/bin
export ROOT=`pwd`/src/github.com/verizonlabs/northstar
export MAKEPATH=$ROOT/make
export BASEMAKEPATH=$ROOT/launcher/make
#
# LOGGER_TAG (DockerImage tag for services)
export LOGGER_TAG=0.3-2.4

# Deploy
export DEPLOY_DIR=$ROOT/launcher/deploy

# Path
export GROUP=northstar
export FGROUP=northstar
export PATH=$PATH:$GOPATH/bin
